<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="w2.com.br.easy_game_app.MenuInicialActivity">

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cad Usuario"
        android:id="@+id/cad_usuario"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="76dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="89dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cad Equipe"
        android:id="@+id/cad_equipe"
        android:layout_marginTop="36dp"
        android:layout_below="@+id/cad_usuario"
        android:layout_alignLeft="@+id/cad_usuario"
        android:layout_alignStart="@+id/cad_usuario"
        android:layout_alignRight="@+id/cad_usuario"
        android:layout_alignEnd="@+id/cad_usuario" />
    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Mapa"
        android:id="@+id/mapa_mostra_usuarios"
        android:layout_marginTop="36dp"
        android:layout_below="@+id/cad_equipe"
        android:layout_alignLeft="@+id/cad_equipe"
        android:layout_alignStart="@+id/cad_equipe"
        android:layout_alignRight="@+id/cad_equipe"
        android:layout_alignEnd="@+id/cad_equipe" />
</RelativeLayout>
